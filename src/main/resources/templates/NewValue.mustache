<script type="text/javascript">
    $(function () {
        $('#container').highcharts({
            chart: {
                type: 'area'
            },
            title: {
                text: 'Ашан'
            },
            subtitle: {
                text: 'subtitle'
            },

            yAxis: {
                title: {
                    text: 'Цена'
                }
            },
            xAxis: {
                categories: date
            },
            legend: {
                enabled: true
            },
            plotOptions: {
                area: {
                    // pointStart: 1850,
                    marker: {
                        enabled: false,
                        symbol: 'circle',
                        radius: 2,
                        states: {
                            hover: {
                                enabled: true
                            }
                        }
                    }
                }
            },
            series: [
                {
                    name: 'Мука',
                    data: Ashan.flour
                },
                {
                    name: 'Молоко',
                    data: Ashan.milk
                },
                {
                    name: 'Сахар',
                    data: Ashan.sugar
                },
                {
                    name: 'Яйца',
                    data: Ashan.eggs
                },
                {
                    name: 'Говядина',
                    data: Ashan.beef
                },
                {
                    name: 'Масло растительное',
                    data: Ashan.sunflowerOil
                }
            ]
        });
    });
</script> <!--1-->
