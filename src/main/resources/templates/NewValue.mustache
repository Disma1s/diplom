            sunflowerOil: {{ПятёрочкаМасло растительное}}
        }

        let date = [
            "декабрь 2016", "март 2017", "июнь 2017",
            "сентябрь 2017","декабрь 2017", "март 2018",
            "июнь 2018", "сентябрь 2018","декабрь 2018",
            "март 2019", "июнь 2019", "сентябрь 2019",
